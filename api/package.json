{
  "name": "gymart-api",
  "version": "1.0.0",
  "description": "GymArt API - Express backend",
  "main": "src/index.js",
  "scripts": {
    "start": "node ../debug-env.js && node src/index.js",
    "build": "echo 'Build completed - Express app ready'",
    "dev": "nodemon src/index.js",
    "test": "jest --testPathIgnorePatterns=integration --forceExit",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=test.js",
    "test:all": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "prettier --write src/**/*.js && eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "express",
    "api",
    "gym",
    "fitness"
  ],
  "author": "sergeimlk",
  "license": "MIT"
}
